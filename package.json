{
  "name": "re-life-rpg",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:8000",
  "scripts": {
    "remove:locales": "node -e \"require('fs').rmSync('public/locales', { recursive: true, force: true })\"",
    "rename": "node -e \"require('fs').rename('public/locales/uk-UA', 'public/locales/ua', function(err) { if (err) console.log(err); console.log('Successfully renamed the directory.') })\"",
    "dev:lang": "npm run remove:locales && i18nexus pull && yarn rename && next dev",
    "update:lang": "npm run remove:locales && i18nexus pull && yarn rename",
    "dev": "next dev",
    "build": "next build",
    "start": "npm run remove:locales && i18nexus pull && yarn rename && next start",
    "lint": "next lint",
    "pwa": "next build && next start",
    "static": "next build && next export",
    "serve": "npm run static && next start",
    "sitemap": "next-sitemap"
  },
  "browser": {
    "fs": false,
    "os": false,
    "path": false
  },
  "babelMacros": {
    "twin": {
      "preset": "styled-components",
      "styled": {
        "import": "default",
        "from": "@emotion/styled"
      },
      "css": {
        "import": "css",
        "from": "@emotion/react"
      },
      "global": {
        "import": "Global",
        "from": "@emotion/react"
      }
    }
  },
